{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "style-guide/page-component/TechnoEvents/TechnoMap.jsx",
    "style-guide/components/SignUp/SignUp.jsx"
, "style-guide/components/Esperanza/index.js", "style-guide/components/Abacus/index.js"  ],
  "exclude": ["node_modules"]
}
